<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head.ejs') %>

  <body>
    <%- include('../partials/navbar.ejs') %>

    <div class="index-header-div">
      <h1>All Fruits</h1>
      <a class="add-fruit" href="/fruits/new">Add fruit</a>
    </div>
    <ul>
      <% fruits.forEach((fruit)=> { %>
      <li>
        <a class="fruit-cards" href="/fruits/<%=fruit._id%>">
          <%= fruit.name %>
        </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
